	public static int[] getNFibonacci() {

		int fib[] = new int[10];
		fib[0] = 1;
		fib[1] = 1;

		for (int i = 2; i <= 10; i++) {
			fib[i] = fib[i - 2] + fib[i - 1];
		}
		return fib;

	}
}
